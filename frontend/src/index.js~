let m = require('mithril');
let workSpace = require('./views/workspace');
let nav = require('./views/nav');



let App = {
    view: function() {
        return m('div',{
            style: 'height:100%'
        }, [
            m(nav),
            m(workSpace)
        ]);
    }
};


m.route(document.body, '/', {
    '/': App
});
